{
  "name": "discord-commander",
  "version": "0.1.0",
  "description": "A Discord bot written in NodeJS that is designed to receive and handle commands via Discord messages",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "doc": "doc",
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "discord.js": "^11.4.0",
    "dotenv": "^6.0.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "@types/underscore": "^1.8.8",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "tsc",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "start": "npm run build:live",
    "test": "mocha -r ts-node/register test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DLii-Technologies/discord-commander.git"
  },
  "keywords": [
    "discord",
    "command",
    "bot"
  ],
  "author": "David Ludwig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DLii-Technologies/discord-commander/issues"
  },
  "homepage": "https://github.com/DLii-Technologies/discord-commander#readme"
}
